<?xml version="1.0"?>
<state
	xmlns="urn:com.tcoffman.ttwb.state/v/1.0"
	xmlns:core="urn:com.tcoffman.ttwb.core/v/1.0"
	xmlns:grid="urn:com.tcoffman.ttwb.grid/v/1.0"
	xmlns:m="urn:com.tcoffman.ttwb.model/v/1.0"
	model="m:complete"
	>
	<current-stage ref="m:turn"/>
	<parts>
	    <part prototype-ref="m:x-piece" id="p1"/>
	    <part prototype-ref="m:board" id="b1"/>
	</parts>
	<relationships>
		<rel type="core:location" src="p1/core:bottom" dst="b1/grid:1-1"/>
	</relationships>
</state>