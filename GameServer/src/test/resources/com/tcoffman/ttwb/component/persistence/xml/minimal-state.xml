	xmlns="urn:com.tcoffman.ttwb.state/v/1.0"
	xmlns:core="urn:com.tcoffman.ttwb.core/v/1.0"
	xmlns:grid="urn:com.tcoffman.ttwb.grid/v/1.0"
	xmlns:m="urn:com.tcoffman.ttwb.model/v/1.0"
	model="m:minimal"
	>
	<current-stage ref="m:epilogue"/>
